        "#W5A10\n",
        "def kt(a, b):\n",
        "    if len(a) != len(b):\n",
        "        return False\n",
        "    map_a = {}\n",
        "    map_b = {}\n",
        "    for c1, c2 in zip(a, b):\n",
        "        if c1 in map_b and map_b[c1] != c2:\n",
        "            return False\n",
        "        if c2 in map_a and map_a[c2] != c1:\n",
        "            return False\n",
        "    map_b[c1] = c2\n",
        "    map_a[c2] = c1\n",
        "    return True\n",
        "a, b =map(str,input().split())\n",
        "print(kt(a,b))"
      ],
